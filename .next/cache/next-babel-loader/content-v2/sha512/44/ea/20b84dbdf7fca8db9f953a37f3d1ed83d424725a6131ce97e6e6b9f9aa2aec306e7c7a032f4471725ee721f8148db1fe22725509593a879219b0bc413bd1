Container}>\n                {options\n                    .filter((option) => option.value !== value)\n                    .map((option) => (\n                        <button\n                            key={option.key}\n                            value={option.value}\n                            className={styles.option}\n                            onClick={onChange}\n                        >\n                            {option.value}\n                        </button>\n                    ))}\n            </div>\n        </div>\n    );\n};\n\nexport default Dropdown;\n"]},"metadata":{},"sourceType":"module"}